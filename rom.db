T-081326 {
	name NBA Jam
	EEPROM {
		type i2c
		size 256
	}
	map {
		0 {
			device ROM
			last 1FFFFF
		}
		200000 {
			device EEPROM
			last 3FFFFF
			bits_read {
				1 sda
			}
			bits_write {
				0 sda
				1 scl
			}
		}
	}
}
